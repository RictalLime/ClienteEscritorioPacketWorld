<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clienteescritoriopacketworld.FXMLModuloClientesController">
   <children>
      <Pane prefHeight="60.0" prefWidth="633.0" style="-fx-background-color: blue;">
         <children>
            <ImageView fitHeight="53.0" fitWidth="54.0" layoutX="290.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@recursos/Packet%20World%20logo%20blanco.png" />
               </image>
            </ImageView>
            <ImageView fx:id="imgRegresar" fitHeight="53.0" fitWidth="43.0" layoutX="26.0" layoutY="9.0" onMouseClicked="#regresarPrincipal" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@recursos/arrow_back.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Label layoutX="45.0" layoutY="104.0" text="Clientes" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="tfBuscar" layoutX="45.0" layoutY="144.0" prefHeight="25.0" prefWidth="205.0" promptText="busca cliente por nombre, núm de teléfono o correo. " style="-fx-border-color: blue;" />
      <ImageView fx:id="imgBuscar" fitHeight="33.0" fitWidth="28.0" layoutX="256.0" layoutY="143.0" onMouseClicked="#irBuscar" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@recursos/search_black.png" />
         </image>
      </ImageView>
      <TableView fx:id="tvTablaClientes" layoutX="21.0" layoutY="187.0" prefHeight="200.0" prefWidth="591.0" style="-fx-border-color: blue;">
         <columns>
            <TableColumn fx:id="colNombre" prefWidth="59.0" text="Nombre" />
            <TableColumn fx:id="colApellidoPaterno" prefWidth="149.0" text="Apellido Paterno" />
            <TableColumn fx:id="colApellidoMaterno" prefWidth="141.0" text="Apellido Materno" />
            <TableColumn fx:id="colTelefono" prefWidth="109.0" text="Telefono" />
            <TableColumn fx:id="colCorreo" prefWidth="129.0" text="Correo" />
         </columns>
      </TableView>
      <Pane layoutX="350.0" layoutY="121.0" prefHeight="53.0" prefWidth="53.0" style="-fx-border-color: blue;">
         <children>
            <ImageView fx:id="imgRegistrarCliente" fitHeight="43.0" fitWidth="53.0" layoutX="5.0" layoutY="5.0" onMouseClicked="#irRegistrarCliente" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@recursos/note_add_blue.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Pane layoutX="432.0" layoutY="121.0" prefHeight="53.0" prefWidth="53.0" style="-fx-border-color: blue;">
         <children>
            <ImageView fx:id="imgEditarCliente" fitHeight="43.0" fitWidth="43.0" layoutX="5.0" layoutY="5.0" onMouseClicked="#irEditarCliente" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@recursos/edit_document_blue.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Pane layoutX="518.0" layoutY="121.0" prefHeight="53.0" prefWidth="53.0" style="-fx-border-color: blue;">
         <children>
            <ImageView fx:id="imgElminarCliente" fitHeight="43.0" fitWidth="43.0" layoutX="5.0" layoutY="5.0" onMouseClicked="#irEliminarCliente" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@recursos/scan_delete_blue.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
   </children>
</AnchorPane>
